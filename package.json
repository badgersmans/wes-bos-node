{
  "name": "dang-thats-delicious",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "start": "set NODE_ENV=production&&node start",
    "dev": "node start.js",
    "seeder": "node ./data/load-sample-data.js -d&&node ./data/load-sample-data.js -i&&node app",
    "prod": "node ./start.js",
    "watch": "nodemon ./start.js --ignore public/",
    "assets": "webpack -w --display-max-modules 0",
    "now": "now -e DB_USER=@db_user -e DB_PASS=@db_pass -e NODE_ENV=\"production\" -e PORT=80"
  },
  "browserslist": "last 3 versions",
  "dependencies": {
    "axios": "0.19.2",
    "connect-flash": "0.1.1",
    "connect-mongo": "3.2.0",
