    "dompurify": "2.0.12",
    "dotenv": "8.2.0",
    "es6-promisify": "6.1.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "express-validator": "^5.3.1",
    "faker": "4.1.0",
    "forever": "3.0.0",
    "glob": "7.1.6",
    "html-to-text": "5.1.1",
    "jimp": "0.14.0",
    "juice": "7.0.0",
    "md5": "2.2.1",
    "moment": "2.27.0",
    "mongoose": "5.9.25",
    "mongoose-mongodb-errors": "0.0.2",
    "multer": "1.4.2",
